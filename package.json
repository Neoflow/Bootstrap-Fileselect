{
    "name": "@neoflow/bootstrap-fileselect",
    "version": "3.1.1",
    "scripts": {
        "js:build": "gulp js:build",
        "js:minify": "gulp js:minify",
        "js:rebuild": "gulp js:rebuild",
        "src:rebuild": "gulp src:rebuild",
        "src:watch": "gulp src:watch"
    },
    "keywords": [
        "file select",
        "input file",
        "form control",
        "bootstrap",
        "jquery"
    ],
    "scope": "neoflow",
    "author": "Jonathan Nessier, Neoflow",
    "license": "MIT",
    "contributors": [],
    "publishConfig": {
        "access": "public"
    },
    "buildConfig": {
        "common": {
            "sourceHeader": "./src/source-header.txt"
        },
        "jsBuild": {
            "src": "./src/js/bootstrap-fileselect.js",
            "dest": "./dist",
            "concatName": "bootstrap-fileselect.js"
        },
        "jsMinify": {
            "src": "./dist/bootstrap-fileselect.js",
            "dest": "./dist",
            "renameSuffix": ".min"
        },
        "srcWatch": {
            "path": "./src/js/**/*.js"
        }
    },
    "devDependencies": {
        "gulp": "^4.0.2",
        "gulp-concat": "^2.6.1",
        "gulp-inject-string": "^1.1.2",
        "gulp-rename": "^2.0.0",
        "gulp-replace": "^1.0.0",
        "gulp-terser": "^1.4.1",
        "jquery": "^3.5.1"
    },
    "dependencies": {}
}
